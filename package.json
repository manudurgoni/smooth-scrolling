{
  "name": "smooth-scrolling",
  "version": "2.3.8",
  "description": "Smooth is a small JavaScript module based on VirtualScroll to create smooth scrolling and parallax effects on scroll.",
  "main": "lib/smooth-scrolling.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "demo": "browser-sync start --no-ghost-mode --server './' --startPath './demo' --serveStatic 'demo' --files 'demo, lib'",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "dependencies": {
    "dom-classes": "github:npm-dom/dom-classes",
    "dom-create-element": "^1.0.2",
    "dom-events": "^0.1.1",
    "prefix": "^1.0.0",
    "virtual-scroll": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "browser-sync": "^2.18.12",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manudurgoni/hover-on-touch.git"
  },
  "author": "Baptiste Briel",
  "license": "MIT"
}
